# srun -l -n 64 --cpus-per-task 32 --ntasks-per-node 4 --gpus-per-node 4 python gcn_15d.py --dataset reddit --batch-size 256 --samp-num 10 --n-bulkmb 599 --replication 2 --sample-method sage --timing --baseline --hostname $hostname &> tmp_reddit_sage_bs256sn10_mb599_p64c2_noselect_noloops.out
# 
# srun -l -n 64 --cpus-per-task 32 --ntasks-per-node 4 --gpus-per-node 4 python gcn_15d.py --dataset reddit --batch-size 256 --samp-num 10 --n-bulkmb 599 --replication 4 --sample-method sage --timing --baseline --hostname $hostname &> tmp_reddit_sage_bs256sn10_mb599_p64c4_noselect_noloops.out
# 
# srun -l -n 64 --cpus-per-task 32 --ntasks-per-node 4 --gpus-per-node 4 python gcn_15d.py --dataset reddit --batch-size 256 --samp-num 10 --n-bulkmb 599 --replication 8 --sample-method sage --timing --baseline --hostname $hostname &> tmp_reddit_sage_bs256sn10_mb599_p64c8_noselect_noloops.out

# Amazon
