#SBATCH --account=m1982_g

hostname=$(scontrol show hostnames | head -n 1)
echo $hostname
# # Reddit
# srun -l -n 64 --cpus-per-task 32 --ntasks-per-node 4 --gpus-per-node 4 python gcn_15d.py --dataset reddit --batch-size 256 --samp-num 10 --n-bulkmb 599 --replication 1 --sample-method sage --timing --baseline --hostname $hostname &> tmp_reddit_sage_bs256sn10_mb599_p64c1_noselect_noloops.out
